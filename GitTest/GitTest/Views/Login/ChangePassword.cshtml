@model GitTest.ViewModel.HomeVMs.ChangePasswordVM

@(
 UI.Form().Size(500, null).DefaultLabelWidth(100).Items(
        UI.TextBox(x => x.ITCode).ReadOnly(true),
        UI.TextBox(x=>x.OldPassword).InputType("password"),
        UI.TextBox(x=>x.NewPassword).InputType("password"),
        UI.TextBox(x=>x.NewPasswordComfirm).InputType("password")
    )
    .Buttons(
        UI.SubmitButton(),
        UI.CloseButton()
    ).RenderHtml()
)